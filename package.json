    "url": "https://github.com/aXises/MrFortnite/issues"
  },
  "homepage": "https://github.com/aXises/MrFortnite#readme",
  "dependencies": {
    "cross-env": "^5.2.1",
    "linkify": "^0.2.1",
    "linkifyjs": "^2.1.9",
    "nikku": "^2.7.2",
    "typescript": "^3.6.3"
  }
}
